<script>
 function createFunction(){
     var result=new Array();
　　for(var i=0;i<10;i++){
　　result[i]=function(num){
　　      return function(){
　　          return num;
　　      };
　　}(i);
　　}
　　return result;
}
var funcs=createFunction();
for(var i=0;i<funcs.length;i++){
　　document.write(funcs[i]()+'<br />');//分别输出0、1、2...9
}
</script>